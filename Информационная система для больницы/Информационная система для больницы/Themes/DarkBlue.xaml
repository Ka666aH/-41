<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <BitmapImage x:Key="logo" UriSource="pack://application:,,,/Images/whitehands.png"/>
    <BitmapImage x:Key="info" UriSource="pack://application:,,,/Images/whiteboldinfo.png"/>
    <BitmapImage x:Key="minus" UriSource="pack://application:,,,/Images/whiteboldminus.png"/>
    <BitmapImage x:Key="square" UriSource="pack://application:,,,/Images/whiteboldsquare.png"/>
    <BitmapImage x:Key="cross" UriSource="pack://application:,,,/Images/whiteboldcross.png"/>

    <SolidColorBrush x:Key="backgroundColor" Color="#7E6EB3"></SolidColorBrush>
    <SolidColorBrush x:Key="menuColor" Color="#4E4274"></SolidColorBrush>
    <SolidColorBrush x:Key="pageColor" Color="#7865B3"></SolidColorBrush>
    <SolidColorBrush x:Key="buttonColor" Color="#534095"></SolidColorBrush>
    <SolidColorBrush x:Key="textColor" Color="#FFFFFF"></SolidColorBrush>

    <Color x:Key="background">#7E6EB3</Color>
    <Color x:Key="menu">#4E4274</Color>
    <Color x:Key="page">#7865B3</Color>
    <Color x:Key="button">#534095</Color>
    <Color x:Key="text">#FFFFFF</Color>

</ResourceDictionary>