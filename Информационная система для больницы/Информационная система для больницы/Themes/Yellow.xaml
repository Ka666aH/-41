<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <BitmapImage x:Key="logo" UriSource="pack://application:,,,/Images/hands.png"/>
    <BitmapImage x:Key="info" UriSource="pack://application:,,,/Images/boldinfo.png"/>
    <BitmapImage x:Key="minus" UriSource="pack://application:,,,/Images/boldminus.png"/>
    <BitmapImage x:Key="square" UriSource="pack://application:,,,/Images/boldsquare.png"/>
    <BitmapImage x:Key="cross" UriSource="pack://application:,,,/Images/boldcross.png"/>

    <SolidColorBrush x:Key="backgroundColor" Color="#E7C683"></SolidColorBrush>
    <SolidColorBrush x:Key="menuColor" Color="#B88F3D"></SolidColorBrush>
    <SolidColorBrush x:Key="pageColor" Color="#E7C176"></SolidColorBrush>
    <SolidColorBrush x:Key="buttonColor" Color="#B88F3D"></SolidColorBrush>
    <SolidColorBrush x:Key="textColor" Color="#000000"></SolidColorBrush>

    <Color x:Key="background">#E7C683</Color>
    <Color x:Key="menu">#B88F3D</Color>
    <Color x:Key="page">#E7C176</Color>
    <Color x:Key="button">#B88F3D</Color>
    <Color x:Key="text">#000000</Color>

</ResourceDictionary>