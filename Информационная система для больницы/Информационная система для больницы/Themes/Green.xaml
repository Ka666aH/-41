<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <BitmapImage x:Key="logo" UriSource="pack://application:,,,/Images/hands.png"/>
    <BitmapImage x:Key="info" UriSource="pack://application:,,,/Images/boldinfo.png"/>
    <BitmapImage x:Key="minus" UriSource="pack://application:,,,/Images/boldminus.png"/>
    <BitmapImage x:Key="square" UriSource="pack://application:,,,/Images/boldsquare.png"/>
    <BitmapImage x:Key="cross" UriSource="pack://application:,,,/Images/boldcross.png"/>

    <SolidColorBrush x:Key="backgroundColor" Color="#F1FFE6"></SolidColorBrush>
    <SolidColorBrush x:Key="menuColor" Color="#357a20"></SolidColorBrush>
    <SolidColorBrush x:Key="pageColor" Color="#64e650"></SolidColorBrush>
    <SolidColorBrush x:Key="buttonColor" Color="#2c3d1f"></SolidColorBrush>
    <SolidColorBrush x:Key="textColor" Color="#000000"></SolidColorBrush>

    <Color x:Key="background">#F1FFE6</Color>
    <Color x:Key="menu">#357a20</Color>
    <Color x:Key="page">#64e650</Color>
    <!--<Color x:Key="page">#b7ff80</Color>-->
    <Color x:Key="button">#2c3d1f</Color>
    <Color x:Key="text">#000000</Color>

</ResourceDictionary>