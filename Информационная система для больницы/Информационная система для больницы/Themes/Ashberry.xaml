<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <BitmapImage x:Key="logo" UriSource="pack://application:,,,/Images/whitehands.png"/>
    <BitmapImage x:Key="info" UriSource="pack://application:,,,/Images/whiteboldinfo.png"/>
    <BitmapImage x:Key="minus" UriSource="pack://application:,,,/Images/whiteboldminus.png"/>
    <BitmapImage x:Key="square" UriSource="pack://application:,,,/Images/whiteboldsquare.png"/>
    <BitmapImage x:Key="cross" UriSource="pack://application:,,,/Images/whiteboldcross.png"/>

    <SolidColorBrush x:Key="backgroundColor" Color="#793E3E"></SolidColorBrush>

    <LinearGradientBrush x:Key="pageColor" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Color="#FF1838" Offset="0.0"/>
        <GradientStop Color="#FFD750" Offset="1.0"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="menuColor" Color="#A60000"></SolidColorBrush>
    <SolidColorBrush x:Key="buttonColor" Color="#A60000"></SolidColorBrush>
    <SolidColorBrush x:Key="textColor" Color="#FFD1D1"></SolidColorBrush>

    <Color x:Key="background">#793E3E</Color>
    <Color x:Key="menu">#A60000</Color>
    <Color x:Key="page">#FF1838</Color>
    <Color x:Key="button">#A60000</Color>
    <Color x:Key="text">#FFD1D1</Color>

</ResourceDictionary>