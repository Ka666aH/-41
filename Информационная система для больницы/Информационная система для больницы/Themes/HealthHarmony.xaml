<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <BitmapImage x:Key="logo" UriSource="pack://application:,,,/Images/hands.png"/>
    <BitmapImage x:Key="info" UriSource="pack://application:,,,/Images/boldinfo.png"/>
    <BitmapImage x:Key="minus" UriSource="pack://application:,,,/Images/boldminus.png"/>
    <BitmapImage x:Key="square" UriSource="pack://application:,,,/Images/boldsquare.png"/>
    <BitmapImage x:Key="cross" UriSource="pack://application:,,,/Images/boldcross.png"/>

    <SolidColorBrush x:Key="backgroundColor" Color="#E8F5E9"></SolidColorBrush>
    <SolidColorBrush x:Key="menuColor" Color="#C8E6C9"></SolidColorBrush>
    <SolidColorBrush x:Key="pageColor" Color="#F1F8E9"></SolidColorBrush>
    <SolidColorBrush x:Key="buttonColor" Color="#81C784"></SolidColorBrush>
    <SolidColorBrush x:Key="textColor" Color="#212121"></SolidColorBrush>

    <Color x:Key="background">#E8F5E9</Color>
    <Color x:Key="menu">#C8E6C9</Color>
    <Color x:Key="page">#F1F8E9</Color>
    <Color x:Key="button">#81C784</Color>
    <Color x:Key="text">#212121</Color>

</ResourceDictionary>