<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <BitmapImage x:Key="logo" UriSource="pack://application:,,,/Images/hands.png"/>
    <BitmapImage x:Key="info" UriSource="pack://application:,,,/Images/boldinfo.png"/>
    <BitmapImage x:Key="minus" UriSource="pack://application:,,,/Images/boldminus.png"/>
    <BitmapImage x:Key="square" UriSource="pack://application:,,,/Images/boldsquare.png"/>
    <BitmapImage x:Key="cross" UriSource="pack://application:,,,/Images/boldcross.png"/>

    <SolidColorBrush x:Key="backgroundColor" Color="#B66291"></SolidColorBrush>
    <SolidColorBrush x:Key="menuColor" Color="#A2487A"></SolidColorBrush>
    <SolidColorBrush x:Key="pageColor" Color="#AE5587"></SolidColorBrush>
    <SolidColorBrush x:Key="buttonColor" Color="#A2487A"></SolidColorBrush>
    <SolidColorBrush x:Key="textColor" Color="#000000"></SolidColorBrush>

    <Color x:Key="background">#B66291</Color>
    <Color x:Key="menu">#A2487A</Color>
    <Color x:Key="page">#AE5587</Color>
    <Color x:Key="button">#A2487A</Color>
    <Color x:Key="text">#000000</Color>

</ResourceDictionary>