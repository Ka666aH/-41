<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <BitmapImage x:Key="logo" UriSource="pack://application:,,,/Images/hands.png"/>
    <BitmapImage x:Key="info" UriSource="pack://application:,,,/Images/boldinfo.png"/>
    <BitmapImage x:Key="minus" UriSource="pack://application:,,,/Images/boldminus.png"/>
    <BitmapImage x:Key="square" UriSource="pack://application:,,,/Images/boldsquare.png"/>
    <BitmapImage x:Key="cross" UriSource="pack://application:,,,/Images/boldcross.png"/>

    <SolidColorBrush x:Key="backgroundColor" Color="#EBB3A8"></SolidColorBrush>
    <SolidColorBrush x:Key="menuColor" Color="#E5AA9A"></SolidColorBrush>
    <SolidColorBrush x:Key="pageColor" Color="#EBADA8"></SolidColorBrush>
    <SolidColorBrush x:Key="buttonColor" Color="#B84B3D"></SolidColorBrush>
    <SolidColorBrush x:Key="textColor" Color="#000000"></SolidColorBrush>

    <Color x:Key="background">#EBB3A8</Color>
    <Color x:Key="menu">#E5AA9A</Color>
    <Color x:Key="page">#EBADA8</Color>
    <Color x:Key="button">#B84B3D</Color>
    <Color x:Key="text">#000000</Color>

</ResourceDictionary>