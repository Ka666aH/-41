<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <BitmapImage x:Key="logo" UriSource="pack://application:,,,/Images/whitehands.png"/>
    <BitmapImage x:Key="info" UriSource="pack://application:,,,/Images/whiteboldinfo.png"/>
    <BitmapImage x:Key="minus" UriSource="pack://application:,,,/Images/whiteboldminus.png"/>
    <BitmapImage x:Key="square" UriSource="pack://application:,,,/Images/whiteboldsquare.png"/>
    <BitmapImage x:Key="cross" UriSource="pack://application:,,,/Images/whiteboldcross.png"/>

    <SolidColorBrush x:Key="backgroundColor" Color="#4E4A59"></SolidColorBrush>
    <SolidColorBrush x:Key="menuColor" Color="#2A2732"></SolidColorBrush>
    <SolidColorBrush x:Key="pageColor" Color="#3C3945"></SolidColorBrush>
    <SolidColorBrush x:Key="buttonColor" Color="#211D2B"></SolidColorBrush>
    <SolidColorBrush x:Key="textColor" Color="White"></SolidColorBrush>

    <Color x:Key="background">#3E3A49</Color>
    <Color x:Key="menu">#2A2732</Color>
    <Color x:Key="page">#3C3945</Color>
    <Color x:Key="button">#211D2B</Color>
    <Color x:Key="text">White</Color>

</ResourceDictionary>