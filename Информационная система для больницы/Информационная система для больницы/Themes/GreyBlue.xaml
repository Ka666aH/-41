<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <BitmapImage x:Key="logo" UriSource="pack://application:,,,/Images/whitehands.png"/>
    <BitmapImage x:Key="info" UriSource="pack://application:,,,/Images/whiteboldinfo.png"/>
    <BitmapImage x:Key="minus" UriSource="pack://application:,,,/Images/whiteboldminus.png"/>
    <BitmapImage x:Key="square" UriSource="pack://application:,,,/Images/whiteboldsquare.png"/>
    <BitmapImage x:Key="cross" UriSource="pack://application:,,,/Images/whiteboldcross.png"/>

    <SolidColorBrush x:Key="backgroundColor" Color="#5DA4A4"></SolidColorBrush>
    <SolidColorBrush x:Key="menuColor" Color="#246E6E"></SolidColorBrush>
    <SolidColorBrush x:Key="pageColor" Color="#468e8e"></SolidColorBrush>
    <SolidColorBrush x:Key="buttonColor" Color="#3B8686"></SolidColorBrush>
    <SolidColorBrush x:Key="textColor" Color="White"></SolidColorBrush>

    <Color x:Key="background">#5DA4A4</Color>
    <Color x:Key="menu">#246E6E</Color>
    <Color x:Key="page">#468e8e</Color>
    <Color x:Key="button">#3B8686</Color>
    <Color x:Key="text">White</Color>

</ResourceDictionary>