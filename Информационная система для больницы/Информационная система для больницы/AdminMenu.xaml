<UserControl x:Class="Информационная_система_для_больницы.AdminMenu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Информационная_система_для_больницы"
             mc:Ignorable="d" 
             d:DesignHeight="260" d:DesignWidth="260">
    <StackPanel x:Name="nurseMenu" VerticalAlignment="Top">

        <RadioButton x:Name="adminEmployees" Content="Сотрудники" Style="{StaticResource menuButtonsStyle}" Margin="0 -1 0 0" Click="adminEmployees_Click"/>

    </StackPanel>
</UserControl>
